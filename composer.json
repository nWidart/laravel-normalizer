{
    "name": "nwidart/laravel-normalizer",
    "type": "library",
    "description": "Laravel package to normalize your data before saving into the database.",
    "keywords": [
        "nwidart",
        "laravel-normalizer",
        "laravel"
    ],
    "homepage": "https://github.com/nwidart/laravel-normalizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "https://nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/database": "5.1.*|5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Nwidart\\LaravelNormalizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nwidart\\LaravelNormalizer\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
